[run]
source = showstock
data_file = /app/.coverage
omit =
    */tests/*
    */migrations/*
    */settings/*
    */__init__.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError

[xml]
output = /app/coverage.xml
